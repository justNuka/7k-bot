{
  "name": "7k-bot",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=20.10"
  },
  "scripts": {
    "register:dev": "node -r dotenv/config -r ./scripts/env/dev.cjs ./node_modules/tsx/dist/cli.mjs src/scripts/register-commands.ts",
    "dev": "node -r dotenv/config -r ./scripts/env/dev.cjs ./node_modules/tsx/dist/cli.mjs src/index.ts",
    "register:prod": "node -r dotenv/config ./node_modules/tsx/dist/cli.mjs src/scripts/register-commands.ts",
    "prod": "node -r dotenv/config ./node_modules/tsx/dist/cli.mjs src/index.ts",
    "start:daemon": "node -r dotenv/config ./node_modules/tsx/dist/cli.mjs src/index.ts",
    "inspect:dev": "node -r dotenv/config -r ./scripts/env/dev.cjs ./node_modules/tsx/dist/cli.mjs src/scripts/inspect-commands.ts",
    "clear-global": "node -r dotenv/config ./node_modules/tsx/dist/cli.mjs src/scripts/clear-global.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node -r dotenv/config dist/index.js",
    "db:init": "node src/scripts/init_db.cjs",
    "db:explorer": "node src/scripts/db_explorer.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justNuka/7k-bot.git"
  },
  "author": "justNuka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justNuka/7k-bot/issues"
  },
  "homepage": "https://github.com/justNuka/7k-bot#readme",
  "description": "A Discord bot for 7k",
  "dependencies": {
    "axios": "^1.11.0",
    "better-sqlite3": "^12.4.1",
    "cheerio": "^1.1.2",
    "cli-table3": "^0.6.5",
    "dayjs": "^1.11.18",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "inquirer": "^12.10.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
